{
  "name": "echotune-ai-production",
  "version": "2.1.0", 
  "description": "EchoTune AI - Production optimized build (excludes dev tools, coding agents, heavy ML)",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:original": "node src/index.js",
    "health-check": "curl -f http://localhost:3000/health || exit 1",
    "logs": "tail -f logs/app.log",
    "restart": "pm2 restart echotune-ai || npm start"
  },
  "keywords": [
    "music",
    "recommendation", 
    "spotify",
    "ai",
    "production",
    "optimized"
  ],
  "author": "EchoTune AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dzp5103/Spotify-echo.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@supabase/supabase-js": "^2.53.0",
    "axios": "^1.5.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.3.0",
    "multer": "^2.0.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.24.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "ws": "^8.14.0"
  },
  "optionalDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.7.1"
  },
  "excludedDependencies": {
    "comment": "These dependencies are intentionally excluded for production optimization",
    "devTools": [
      "@babel/core",
      "@babel/preset-env", 
      "@types/node",
      "eslint",
      "prettier",
      "jest",
      "nodemon",
      "typescript",
      "webpack"
    ],
    "codingAgents": [
      "@browserbasehq/mcp-server-browserbase",
      "@modelcontextprotocol/server-filesystem",
      "@modelcontextprotocol/server-sequential-thinking",
      "FileScopeMCP",
      "mcp-mermaid",
      "puppeteer",
      "playwright"
    ],
    "heavyML": [
      "pandas",
      "numpy",
      "scikit-learn",
      "matplotlib",
      "seaborn",
      "plotly",
      "jupyter",
      "librosa"
    ],
    "browserAutomation": [
      "puppeteer",
      "playwright",
      "@playwright/test",
      "chromium"
    ]
  },
  "production": {
    "optimization": "minimal",
    "excludes": [
      "dev-dependencies",
      "coding-agents", 
      "heavy-ml-packages",
      "browser-automation",
      "testing-tools",
      "build-tools"
    ],
    "resourceUsage": "minimal",
    "securityHardened": true
  }
}